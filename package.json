{
  "name": "superbot",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=development nodemon",
    "lint": "eslint --ext .ts ./",
    "pre-commit-message": "echo 'Please wait, running pre-commit checks...' && exit 0",
    "test": "echo \"No tests specified!\" && exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit-message && npm run lint && npm test"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "mutablestate.js": "^0.2.6",
    "node-telegram-bot-api": "^0.40.0",
    "puppeteer": "^2.0.0",
    "twilio": "^3.37.1",
    "ussd-router": "^0.1.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.14",
    "@types/node-telegram-bot-api": "^0.40.0",
    "@types/puppeteer": "^2.0.0",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.1.0",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.3",
    "typescript": "^3.7.2"
  }
}
